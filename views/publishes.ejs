<%- include("_header") -%>

    <br>
       <h2> Reader's Page </h2> 
    <br>

    <div class="table-responsive-lg">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Author</th>
                    <th>Article Title</th>
                    <th>Article Subtitle</th>
                </tr>
            </thead>
            <tbody>
                <% for (const publish of model) { %>
                    <tr>
                        <td>
                            <%= publish.ID %>
                        </td>
                        <td>
                            <%= publish.Author %>
                        </td>
                        <td>
                            <%= publish.Title %>
                        </td>
                        <td>
                            <%= publish.Subtitle %>
                        </td>
                        <td class="d-print-none">
                            <a class="btn btn-default btn-primary" href="/user/read/<%= publish.ID %>">Read</a>
                        </td>
                    </tr>
                    <% } %>
            </tbody>
        </table>
    </div>

    <br>
    <br>
    <br>
    <br>
    <br>
    <br>

    <%- include("_footer") -%>